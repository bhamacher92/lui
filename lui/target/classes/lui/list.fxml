<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<VBox fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
    fx:controller="lui.ListController">
    <children>
         <ListView VBox.vgrow="ALWAYS" fx:id="listView">
         </ListView>
         <HBox >
         <TextField HBox.hgrow="ALWAYS" fx:id="input" onKeyPressed="#handleOnKeyPressed" />
         <Button fx:id="addButton" text="Add" onAction="#handleAddElement" />
         </HBox>
    </children>
</VBox>