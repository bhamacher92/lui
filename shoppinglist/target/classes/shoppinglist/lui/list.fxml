<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<VBox fx:id="root" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
    <children>
         <HBox>
         <Button fx:id="backButton" text=".." onAction="#handleBackToStart"/>
         <TextField fx:id="titel" HBox.hgrow="ALWAYS" text="#unnamed" />
         </HBox>
         <ListView VBox.vgrow="ALWAYS" fx:id="listView">
         </ListView>
         <HBox >
         <TextField HBox.hgrow="ALWAYS" fx:id="input" onKeyPressed="#handleOnKeyPressed" />
         <Button fx:id="addButton" text=" + " onAction="#handleAddElement" />
         </HBox>
    </children>
</VBox>